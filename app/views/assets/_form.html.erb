
  <%= form_for @asset, :html => {:multipart => true} do |f| %>  
       <p>
       <%= @asset.file_name %>  
           <%= f.label :upload_file, "File" %><br>  
           <%= f.file_field :upload_file %>  
         </p>  
 <%= f.select(:level_id, Level.all.collect {|a| [a.name, a.id]})%>
  <div class="field">
    <%#= f.label :user_id %><br>
    <%= f.hidden_field :folder_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
